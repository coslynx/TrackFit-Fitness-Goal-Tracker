# Next.js public API URL (for client-side requests)
NEXT_PUBLIC_API_URL=http://localhost:3000

# Placeholder for database connection string (will be replaced with the actual URL in production)
DATABASE_URL=postgres://user:password@host:port/database

# Placeholder for Google Client ID (if Google authentication is implemented)
GOOGLE_CLIENT_ID=YOUR_GOOGLE_CLIENT_ID_HERE

# Placeholder for Stripe publishable key (if Stripe payment processing is implemented)
STRIPE_PUBLISHABLE_KEY=YOUR_STRIPE_PUBLISHABLE_KEY_HERE

# Secret key for Next.js session library
SESSION_SECRET=YOUR_SESSION_SECRET_HERE